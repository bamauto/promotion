{
  "rewrites": [
    {
      "source": "/_astro/:path*",
      "destination": "https://blog-seven-gilt-10.vercel.app/_astro/:path*"
    },
    {
      "source": "/blog",
      "destination": "https://blog-seven-gilt-10.vercel.app/jengja"
    },
    {
      "source": "/blog/:path*",
      "destination": "https://blog-seven-gilt-10.vercel.app/jengja/:path*"
    },
    {
      "source": "/jengja",
      "destination": "https://blog-seven-gilt-10.vercel.app/jengja"
    },
    {
      "source": "/jengja/:path*",
      "destination": "https://blog-seven-gilt-10.vercel.app/jengja/:path*"
    },
    {
      "source": "/sitemap-blog.xml",
      "destination": "https://blog-seven-gilt-10.vercel.app/jengja/sitemap.xml"
    },
    {
      "source": "/((?!blog|sitemap-blog|jengja|_astro).*)",
      "destination": "/index.html"
    }
  ]
}
