{
  "rewrites": [
    {
      "source": "/blog",
      "destination": "https://promotion-blog.vercel.app/ingedong"
    },
    {
      "source": "/blog/:path*",
      "destination": "https://promotion-blog.vercel.app/ingedong/:path*"
    },
    {
      "source": "/sitemap-blog.xml",
      "destination": "https://promotion-blog.vercel.app/ingedong/sitemap.xml"
    },
    {
      "source": "/((?!blog|sitemap-blog).*)",
      "destination": "/index.html"
    }
  ]
}
