{
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/_astro/:path*",
      "destination": "https://blog-seven-gilt-10.vercel.app/_astro/:path*"
    },
    {
      "source": "/blog",
      "destination": "https://blog-seven-gilt-10.vercel.app/suwon"
    },
    {
      "source": "/blog/:path*",
      "destination": "https://blog-seven-gilt-10.vercel.app/suwon/:path*"
    },
    {
      "source": "/suwon",
      "destination": "https://blog-seven-gilt-10.vercel.app/suwon"
    },
    {
      "source": "/suwon/:path*",
      "destination": "https://blog-seven-gilt-10.vercel.app/suwon/:path*"
    },
    {
      "source": "/sitemap-blog.xml",
      "destination": "https://blog-seven-gilt-10.vercel.app/suwon/sitemap.xml"
    },
    {
      "source": "/((?!blog(?:/|$)|sitemap-blog|suwon(?:/|$)|_astro(?:/|$)).*)",
      "destination": "/index.html"
    }
  ]
}
