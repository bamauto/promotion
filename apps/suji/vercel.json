{
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/_astro/:path*",
      "destination": "https://blog-seven-gilt-10.vercel.app/_astro/:path*"
    },
    {
      "source": "/blog",
      "destination": "https://blog-seven-gilt-10.vercel.app/suji"
    },
    {
      "source": "/blog/:path*",
      "destination": "https://blog-seven-gilt-10.vercel.app/suji/:path*"
    },
    {
      "source": "/suji",
      "destination": "https://blog-seven-gilt-10.vercel.app/suji"
    },
    {
      "source": "/suji/:path*",
      "destination": "https://blog-seven-gilt-10.vercel.app/suji/:path*"
    },
    {
      "source": "/sitemap-blog.xml",
      "destination": "https://blog-seven-gilt-10.vercel.app/suji/sitemap.xml"
    },
    {
      "source": "/((?!blog(?:/|$)|sitemap-blog|suji(?:/|$)|_astro(?:/|$)).*)",
      "destination": "/index.html"
    }
  ]
}
