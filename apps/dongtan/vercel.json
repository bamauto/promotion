{
  "rewrites": [
    {
      "source": "/_astro/:path*",
      "destination": "https://blog-seven-gilt-10.vercel.app/_astro/:path*"
    },
    {
      "source": "/blog",
      "destination": "https://blog-seven-gilt-10.vercel.app/dongtan"
    },
    {
      "source": "/blog/:path*",
      "destination": "https://blog-seven-gilt-10.vercel.app/dongtan/:path*"
    },
    {
      "source": "/dongtan",
      "destination": "https://blog-seven-gilt-10.vercel.app/dongtan"
    },
    {
      "source": "/dongtan/:path*",
      "destination": "https://blog-seven-gilt-10.vercel.app/dongtan/:path*"
    },
    {
      "source": "/sitemap-blog.xml",
      "destination": "https://blog-seven-gilt-10.vercel.app/dongtan/sitemap.xml"
    },
    {
      "source": "/((?!blog|sitemap-blog|dongtan|_astro).*)",
      "destination": "/index.html"
    }
  ]
}
