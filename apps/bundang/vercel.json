{
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/_astro/:path*",
      "destination": "https://blog-seven-gilt-10.vercel.app/_astro/:path*"
    },
    {
      "source": "/blog",
      "destination": "https://blog-seven-gilt-10.vercel.app/bundang"
    },
    {
      "source": "/blog/:path*",
      "destination": "https://blog-seven-gilt-10.vercel.app/bundang/:path*"
    },
    {
      "source": "/bundang",
      "destination": "https://blog-seven-gilt-10.vercel.app/bundang"
    },
    {
      "source": "/bundang/:path*",
      "destination": "https://blog-seven-gilt-10.vercel.app/bundang/:path*"
    },
    {
      "source": "/sitemap-blog.xml",
      "destination": "https://blog-seven-gilt-10.vercel.app/bundang/sitemap.xml"
    },
    {
      "source": "/((?!blog|sitemap-blog|bundang|_astro).*)",
      "destination": "/index.html"
    }
  ]
}
