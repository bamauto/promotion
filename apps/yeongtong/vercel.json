{
  "rewrites": [
    {
      "source": "/_astro/:path*",
      "destination": "https://blog-seven-gilt-10.vercel.app/_astro/:path*"
    },
    {
      "source": "/blog",
      "destination": "https://blog-seven-gilt-10.vercel.app/yeongtong"
    },
    {
      "source": "/blog/:path*",
      "destination": "https://blog-seven-gilt-10.vercel.app/yeongtong/:path*"
    },
    {
      "source": "/yeongtong",
      "destination": "https://blog-seven-gilt-10.vercel.app/yeongtong"
    },
    {
      "source": "/yeongtong/:path*",
      "destination": "https://blog-seven-gilt-10.vercel.app/yeongtong/:path*"
    },
    {
      "source": "/sitemap-blog.xml",
      "destination": "https://blog-seven-gilt-10.vercel.app/yeongtong/sitemap.xml"
    },
    {
      "source": "/((?!blog|sitemap-blog|yeongtong|_astro).*)",
      "destination": "/index.html"
    }
  ]
}
