{
  "rewrites": [
    {
      "source": "/blog",
      "destination": "https://promotion-blog.vercel.app/jengja"
    },
    {
      "source": "/blog/:path*",
      "destination": "https://promotion-blog.vercel.app/jengja/:path*"
    },
    {
      "source": "/sitemap-blog.xml",
      "destination": "https://promotion-blog.vercel.app/jengja/sitemap.xml"
    },
    {
      "source": "/((?!blog|sitemap-blog).*)",
      "destination": "/index.html"
    }
  ]
}
