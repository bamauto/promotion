{
  "rewrites": [
    {
      "source": "/_astro/:path*",
      "destination": "https://blog-seven-gilt-10.vercel.app/_astro/:path*"
    },
    {
      "source": "/blog",
      "destination": "https://blog-seven-gilt-10.vercel.app/ingedong"
    },
    {
      "source": "/blog/:path*",
      "destination": "https://blog-seven-gilt-10.vercel.app/ingedong/:path*"
    },
    {
      "source": "/ingedong",
      "destination": "https://blog-seven-gilt-10.vercel.app/ingedong"
    },
    {
      "source": "/ingedong/:path*",
      "destination": "https://blog-seven-gilt-10.vercel.app/ingedong/:path*"
    },
    {
      "source": "/sitemap-blog.xml",
      "destination": "https://blog-seven-gilt-10.vercel.app/ingedong/sitemap.xml"
    },
    {
      "source": "/((?!blog|sitemap-blog|ingedong).*)",
      "destination": "/index.html"
    }
  ]
}
